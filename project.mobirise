          "_name": "slider1",
          "_sourceTheme": "snackm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"slider1\" group=\"Gallery & Slider\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <input type=\"id\" name=\"sliderId\">\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverse\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Details\" name=\"showDetails\">\n        <input type=\"checkbox\" title=\"Line\" name=\"showLine\" condition=\"showDetails\" checked>\n        <input type=\"color\" title=\"Color\" name=\"lineColor\" value=\"#ffffff\" condition=\"showDetails && showLine\">\n        <input type=\"range\" title=\"Opacity\" name=\"lineOpacity\" min=\"0.1\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showDetails && showLine\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\">\n        <select title=\"Align\" name=\"alignButtons\" condition=\"showButtons\">\n            <option value=\"justify-content-start\" selected>Left</option>\n            <option value=\"justify-content-center\">Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Carousel</header>\n        <input type=\"checkbox\" title=\"Indicators\" name=\"showIndicators\">\n        <input type=\"color\" title=\"Color\" name=\"indicatorsColor\" value=\"#ffffff\" condition=\"showIndicators\">\n        <input type=\"checkbox\" title=\"Controls\" name=\"showControls\" checked>\n        <input type=\"color\" title=\"Color\" name=\"controlsColor\" value=\"#666666\" condition=\"showControls\">\n        <input type=\"color\" title=\"Hover\" name=\"controlsHoverColor\" value=\"#33d5ad\" condition=\"showControls\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked>\n        <input type=\"range\" title=\"Interval\" name=\"interval\" min=\"3\" max=\"10\" value=\"3\" step=\"1\" condition=\"autoplay\">\n        <header>Nutrition</header>\n        <input type=\"checkbox\" title=\"Show\" name=\"showNutrition\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" condition=\"showNutrition\" checked>\n        <input type=\"checkbox\" title=\"Decoration\" name=\"decoration\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"decorationColor\" value=\"#1f1437\" condition=\"showNutrition && decoration\">\n        <header condition=\"showNutrition\">Card Background</header>\n        <input type=\"checkbox\" title=\"1 Transparent\" name=\"cardOneTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardOneBg\" value=\"#1f1437\" condition=\"showNutrition && !cardOneTr\">\n        <input type=\"checkbox\" title=\"2 Transparent\" name=\"cardTwoTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardTwoBg\" value=\"#1f1437\" condition=\"showNutrition && !cardTwoTr\">\n        <input type=\"checkbox\" title=\"3 Transparent\" name=\"cardThreeTr\" condition=\"showNutrition\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardThreeBg\" value=\"#1f1437\" condition=\"showNutrition && !cardThreeTr\">\n        <input type=\"checkbox\" title=\"4 Transparent\" name=\"cardFourTr\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Color\" name=\"cardFourBg\" value=\"#1f1437\" condition=\"showNutrition && !cardFourTr\">\n        <header condition=\"showNutrition\">Card Border</header>\n        <input type=\"color\" title=\"One\" name=\"cardOneBorder\" value=\"#ffffff\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Two\" name=\"cardTwoBorder\" value=\"#ffffff\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Three\" name=\"cardThreeBorder\" value=\"#ffffff\" condition=\"showNutrition\">\n        <input type=\"color\" title=\"Four\" name=\"cardFourBorder\" value=\"#1f1437\" condition=\"showNutrition\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/snackm5/components/_images/background2.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#f7a379\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container-fluid\">\n        <div class=\"row content\">\n            <div class=\"col-12 col-lg-6\">\n                <div id=\"{{sliderId}}\" class=\"carousel slide\" mbr-class=\"{'carousel-fade': fade}\" data-ride=\"{{autoplay ? 'carousel' : false}}\" data-bs-ride=\"{{autoplay ? 'carousel' : false}}\" data-interval=\"{{interval*1000}}\" data-bs-interval=\"{{interval*1000}}\">\n                    <ol class=\"carousel-indicators\" mbr-if=\"showIndicators\">\n                        <li data-target=\"#{{sliderId}}\" data-bs-target=\"#{{sliderId}}\" data-slide-to=\"0\" data-bs-slide-to=\"0\" class=\"active\"></li>\n                        <li data-target=\"#{{sliderId}}\" data-bs-target=\"#{{sliderId}}\" data-slide-to=\"1\" data-bs-slide-to=\"1\"></li>\n                        <li data-target=\"#{{sliderId}}\" data-bs-target=\"#{{sliderId}}\" data-slide-to=\"2\" data-bs-slide-to=\"2\"></li>\n                    </ol>\n                    <div class=\"carousel-inner\">\n                        <div class=\"carousel-item slider-image item active\">\n                            <div class=\"item-wrapper\">\n                                <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/1641787374628-960x1134.jpg\">\n                            </div>\n                        </div>\n                        <div class=\"carousel-item slider-image item\">\n                            <div class=\"item-wrapper\">\n                                <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/1642001620551-713x1007.jpg\">\n                            </div>\n                        </div>\n                        <div class=\"carousel-item slider-image item\">\n                            <div class=\"item-wrapper\">\n                                <img class=\"d-block w-100\" src=\"@PROJECT_PATH@/assets/images/1642001629294-707x992.jpg\">\n                            </div>\n                        </div>\n                    </div>\n                    <a class=\"carousel-control carousel-control-prev\" href=\"#{{sliderId}}\" role=\"button\" data-slide=\"prev\" data-bs-slide=\"prev\" mbr-if=\"showControls\">\n                        <span class=\"mobi-mbri mobi-mbri-arrow-prev\" aria-hidden=\"true\"></span>\n                        <span class=\"sr-only visually-hidden\">Previous</span>\n                    </a>\n                    <a class=\"carousel-control carousel-control-next\" href=\"#{{sliderId}}\" role=\"button\" data-slide=\"next\" data-bs-slide=\"next\" mbr-if=\"showControls\">\n                        <span class=\"mobi-mbri mobi-mbri-arrow-next\" aria-hidden=\"true\"></span>\n                        <span class=\"sr-only visually-hidden\">Next</span>\n                    </a>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-6\">\n                <div class=\"text\">\n                    <div class=\"text-wrapper\">\n                        <h5 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\">\n                            <b>TASTE</b>\n                        </h5>\n                        <h3 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">\n                            <b><br></b><br><br><br><br><b>城市走繪-手繪速寫展覽</b>\n                        </h3>\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">走吧!!!畫吧!!!<br>不要猶豫.只要一張紙一枝筆就可以開始!!!<br>15分鐘可以做什麼?滑手機,看臉書.開IG?<br>畫圖可以很簡單<br>直線畫不直尺不可恥。圓圈畫不圓,快找個杯子不用很厲害才可以開始,只要敢開始就可以很厲害生活美學現在就可以開始!!!<br><br><br>* 建築古蹟：描繪台南各歷史建築<br>* 街道巷弄：深入台南巷弄探尋常民文化<br>* 生活速寫：台南為美食之都，描寫豐富多元的飲食<br>* 繪畫工作室的日常 Open studio</p>\n                    </div>\n                    <div class=\"nutrition\" mbr-if=\"showNutrition\">\n                        <h3 class=\"mbr-card-title mbr-fonts-style\" data-app-selector=\".mbr-card-title\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\">\n                            <b>NUTRITION PER SERVING</b>\n                        </h3>\n                        <div class=\"nutrition-wrap\">\n                            <div class=\"nutrition-tile tile-one\">\n                                <p class=\"mbr-tile-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardTitle\" data-app-selector=\".mbr-tile-text\">PROTEIN</p>\n                                <p class=\"mbr-tile-number mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-tile-number\"><b>12,15</b></p>\n                            </div>\n                            <div class=\"nutrition-tile tile-two\">\n                                <p class=\"mbr-tile-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardTitle\" data-app-selector=\".mbr-tile-text\">FAT</p>\n                                <p class=\"mbr-tile-number mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-tile-number\"><b>12,15</b></p>\n                            </div>\n                            <div class=\"nutrition-tile tile-three\">\n                                <p class=\"mbr-tile-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardTitle\" data-app-selector=\".mbr-tile-text\">CARBS</p>\n                                <p class=\"mbr-tile-number mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-tile-number\"><b>14,5</b></p>\n                            </div>\n                            <div class=\"nutrition-tile tile-four\">\n                                <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 118 104\" fill=\"none\">\n                                    <path d=\"M94.9869 66.7718C91.7751 66.4762 89.049 66.2941 86.3455 65.9583C81.7583 65.4076 77.2524 64.6595 73.176 62.2058C69.0426 59.7122 66.997 56.0506 67.5116 51.3533C67.832 48.4696 68.9341 45.6803 69.7858 42.8855C70.1246 41.7944 70.7765 40.781 71.5171 39.2363C69.158 40.1865 67.3165 40.9617 65.4447 41.6592C60.8779 43.383 56.3777 45.2945 51.7216 46.7015C45.351 48.6617 40.8666 46.0641 39.1207 39.589C38.4379 37.0494 38.3696 34.3178 38.049 31.6642C37.9574 30.8656 37.9815 30.0632 37.9448 29.1052C34.5039 32.2092 31.2774 35.2087 27.9577 38.0584C20.2056 44.7032 13.9137 39.8817 11.7825 33.109C11.2121 31.2978 